{
	"semi": false,
	"singleQuote": true,
	"useTabs": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"bracketSpacing": true,
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "ignore",
	"embeddedLanguageFormatting": "auto",
	"overrides": [
		{
			"files": "*.scss",
			"options": {
				"semi": true
			}
		}
	]
}
